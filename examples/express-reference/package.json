{
  "name": "ter-express-reference",
  "version": "1.0.0",
  "description": "Reference deployment: Express.js app with TER environment validation",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "validate": "ter check --env .env --contract .ter.json",
    "validate:prod": "ter check --env .env.prod --contract .ter.prod.json",
    "test": "echo 'Tests would run here'"
  },
  "dependencies": {
    "express": "^4.18.0",
    "ter": "^1.0.0-rc1"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  }
}
